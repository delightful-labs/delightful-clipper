{
  "name": "delightful-clipper",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@playwright/test": "^1.21.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.51",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@xstate/svelte": "^1.0.0",
    "article-parser": "^6.0.0",
    "epubjs": "^0.3.93",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "pdfjs-dist": "^2.13.216",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "ramda": "^0.28.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-json-schema-to-form": "https://github.com/delightful-labs/svelte-json-schema-to-form.git#ba7d314b5430e04e9afb4ec72aa37c0af7e757d7",
    "svelte-preprocess": "^4.10.1",
    "svelte-tags-input": "^2.9.2",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "uuid": "^8.3.2",
    "xstate": "^4.31.0"
  },
  "type": "module",
  "dependencies": {
    "webnative": "^0.31.1"
  }
}
